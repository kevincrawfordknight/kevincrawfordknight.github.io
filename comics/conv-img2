mkdir -p trimmed
for src in images/*.jpg; do
  # get dimensions
  read W H < <(identify -format "%w %h\n" "$src")
  # convert 126pt â†’ px (at 72dpi: 1pt = 1px; if your images are 96dpi use 1pt=1.333px)
  CX=126
  CY=126
  # new width/height
  NW=$((W - CX))
  NH=$((H - CY))
  convert "$src" -crop ${NW}x${NH}+${CX}+0 +repage "trimmed/${src##*/}"
done
